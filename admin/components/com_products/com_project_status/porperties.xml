        <view_msg name="view_msg"  type="enable" title="view_msg" value='0'  pclass='col-4' />
        <msg_hover name="msg_hover"  type="enable" title="msg_hover" value='0'  pclass='col-4' />
        <view_des name="view_des"  type="enable" title="view_des" value='0'  pclass='col-4' />
        <view_more name="view_more"  type="enable" title="view_more" value='0'  pclass='col-4' />
        <view_don_bar name="view_don_bar"  type="enable" title="view_don_bar" value='0'  pclass='col-4' />
        <view_overlayer name="view_overlayer"  type="enable" title="view_overlayer" value='0'  pclass='col-4' />
       
        
        
        <enabled name="enabled"  type="enable" title="enable" value='1'  pclass='_50' />
        <msg name="msg"  type="editor" title="msg" />
     

    </fields>
    



    
    

      
    <group>
        <com_products_order title="Order"  />
        <com_products title="Products" parent="yes" />
        <com_products_price title=" Price"  />
        <com_products_categories title="Categories "  />
        <plg_shipping title="Shipping"  />
        <plg_shipper title="Shipper"  />
        <plg_coupon title="Create Coupon"  />
        <plg_shipping_setting title="Shipping Setting"  />
        <com_products_setting title=" Setting"  />
        <com_order_status title="Order Status"  />

    </group>
   
    
    
    
       
    <listfields  multiLevel="yes" >   
        <name list-width="6"/>
        <image list-width="2"/>
        <order list-width="1"/>
        <enabled list-width="1"/>
    </listfields>
    
    

</root>


    

