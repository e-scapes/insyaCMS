<?xml version="1.0" encoding="UTF-8"?>
<!--
To change this template, choose Tools | Templates
and open the template in the editor.
-->
<root>
    <fields>
      
        
        <follow name='follow' title='follow' type='select'  select_data="1>>yes;0>>no" pclass='col-4' />
        
           
        <share name='share' title='share' type='select'  select_data="1>>yes;0>>no"  pclass='col-4'  />
              <links name='links' title='links' type='select'  select_data="1>>yes;0>>no"   pclass='col-4' />
      
        
        <follow_options name='follow_options'  collapse="yes" type='block' title="follow Options "   />
        <twitter name='twitter' title='twitter' type='text' />
        <facebook name='facebook' title='facebook' type='text' />
        <google name='google' title='google' type='text' />
        
        <follow_options_end name='follow_options_end' type='endblock'    />
        

            
        <shar_options name='shar_options'  collapse="yes" type='block' title="Share Options "    />
        <shartwitter name='shartwitter' title='twitter' type='select'  select_data="1>>yes;0>>no"  pclass='col-6'  value="1"/>
        <sharfacebook name='sharfacebook' title='facebook' type='select'  select_data="1>>yes;0>>no" pclass='col-6'  value="1"/>
        <shargoogle name='shargoogle' title='google' type='select'  select_data="1>>yes;0>>no"  pclass='col-6'  value="1"/>
        <sharinlink name='sharinlink' title='inlink' type='select'  select_data="1>>yes;0>>no"  pclass='col-6' />
   
        

        <shar_options_end name='shar_options_end' type='endblock'    />
        
        
        
        

        <links_options name='links_options'  collapse="yes" type='block' title="links Options "    />
        <linkstwitter name='linkstwitter' title='twitter' type='text'   pclass='col-6'  />
        <linksfacebook name='linksfacebook' title='facebook' type='text'  pclass='col-6'  />
        <linksgoogle name='linksgoogle' title='google' type='text'    pclass='col-6'  />
        <linksinlink name='linksinlink' title='inlink' type='text'    pclass='col-6' />
   
        

        <links_options_end name='links_options_end' type='endblock'    />
        
        
        
        
    </fields>
</root>


    

